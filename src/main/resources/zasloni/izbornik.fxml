        <MenuItem mnemonicParsing="false" onAction="#prikaziZaslonZaPretraguKorisnika" text="Pretraga i brisanje" />
          <MenuItem mnemonicParsing="false" onAction="#prikaziZaslonZaUnosKorisnika" text="Unos" />
          <MenuItem mnemonicParsing="false" onAction="#prikaziZaslonZaIzmjenuKorisnika" text="Izmjena" />
      </items>
