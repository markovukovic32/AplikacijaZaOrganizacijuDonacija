             <MenuItem mnemonicParsing="false" onAction="#prikaziZaslonZaIzmjenuPredmetaDoniranja" text="Izmjena" />
         </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Promjene">
          <items>
              <MenuItem mnemonicParsing="false" onAction="#prikaziZaslonSPopisomIzmjenjenihPodataka" text="Popis promjena" />
          </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Odjava">
        <items>
          <MenuItem mnemonicParsing="false" text="Odjavite se" onAction="#prikaziZaslonPrijave"/>
        </items>
      </Menu>
