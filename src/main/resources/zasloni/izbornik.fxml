    </Menu>
    <Menu mnemonicParsing="false" text="Donori">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#prikaziZaslonZaPretraguDonora" text="Pretraga i brisanje" />
            <MenuItem mnemonicParsing="false" onAction="#prikaziZaslonZaUnosDonora" text="Unos" />
          <MenuItem mnemonicParsing="false" onAction="#prikaziZaslonZaIzmjenuDonora" text="Izmjena" />
      </items>
    </Menu>
      <Menu mnemonicParsing="false" text="Donacije">
         <items>
            <MenuItem mnemonicParsing="false" onAction="#prikaziZaslonZaPretraguDonacija" text="Pretraga i brisanje" />
            <MenuItem mnemonicParsing="false" onAction="#prikaziZaslonZaUnosDonacija" text="Unos" />
             <MenuItem mnemonicParsing="false" onAction="#prikaziZaslonZaIzmjenuDonacije" text="Izmjena" />
         </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Predmeti doniranja">
         <items>
            <MenuItem mnemonicParsing="false" onAction="#prikaziZaslonZaPretraguPredmetaDoniranja" text="Pretraga i brisanje" />
